{
    "name": "dofy",
    "version": "0.1.1",
    "bin": "lib/index.js",
    "description": "A CLI tool that tells you when your next day off is.",
    "repository": {
        "type": "git",
        "url": "https://github.com/thetnaingtn/isitdayoffyet"
    },
    "keywords": ["github","go", "cli", "bubble", "bubbletea"],
    "license": "MIT OR Apache-2.0",
