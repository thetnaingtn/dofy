        "dev": "pnpm build && node lib/index.js"
    },
    "devDependencies": {
        "@types/node": "^18.19.86",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "typescript": "^4.9.5"
    },
    "optionalDependencies": {
        "dofy-darwin-amd64": "0.1.1",
        "dofy-darwin-arm64": "0.1.1",
        "dofy-linux-amd64": "0.1.1",
        "dofy-linux-arm64": "0.1.1",
        "dofy-windows-amd64": "0.1.1",
        "dofy-windows-arm64": "0.1.1"
    }
}